{
  "name": "@codedocs/theme",
  "version": "0.1.0",
  "description": "Default theme for CodeDocs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./css/*": "./src/css/*"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@codedocs/core": "*",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@codedocs/core": "*",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "files": [
    "dist",
    "src/css"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jay05410/codedocs.git",
    "directory": "packages/theme"
  }
}
